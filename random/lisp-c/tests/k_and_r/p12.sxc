(#include <stdio.h>)

// print Fhrenheit-Celsius table
// for fahr = 0, 20, ..., 300 floating-point version
(int main ((int argc) (char (** argv)))
     (var float fahr celsius)
     (var int lower upper step)

     /* comment */
     (= lower 0)
     (= upper 300)
     (= step 20)

     /* comment */
     (= fahr lower)

     /* comment */
     (while (<= fahr upper)
       (= celsius /* testing */ (* (/ 5.0 9.0) (- fahr 32.0)))
       (printf "%3.0f %6.1f\n" fahr celsius)
       (= fahr (+ fahr step)))
     (return 0))

/* /* comment */ */
