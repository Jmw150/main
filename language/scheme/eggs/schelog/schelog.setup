;;; schelog.setup -*- Scheme -*-

;; XXX no "-local" since schelog:use-occurs-check may be externally redefined
(run (csc -sJ -O2 -specialize -inline-global -d1 schelog-module.scm -o schelog.so))
(run (csc -s -O3 -d0 schelog.import.scm))

(install-extension
 'schelog
 '("schelog.so" "schelog.import.so")
 '((version "3.6")))
