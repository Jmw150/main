;;;; stack.setup -*- Hen -*-

(use setup-helper-mod)

(verify-extension-name 'stack)

(setup-shared+static-extension-module (extension-name) (extension-version "2.3.0")
  #:inline? #t
  #:types? #t
  #:compile-options '(
    -disable-interrupts
    -fixnum-arithmetic
    -optimize-level 3
    -debug-level 2
    -inline-limit 50
    -no-procedure-checks))
